// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Device {
  id             String    @id
  name           String
  cpuName        String
  cpuUsage       Int
  cpuTemperature Int
  gpuName        String
  gpuTemperature Int
  gpuUsage       Int
  ram            Int
  ramUsed        Int
  updatedAt      DateTime? @default(now())
  createdAt      DateTime? @default(now())
}
